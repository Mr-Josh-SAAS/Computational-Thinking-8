{
  "name": "Python 3",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
    "ghcr.io/devcontainers/features/desktop-lite:1": {      
			"password": "",
			"webPort": "6080"
			// "vncPort": "5901"
    }
  },
  "forwardPorts": [
    6080
  ],
  "portsAttributes": {
    "6080": {
      "label": "desktop"
    }
  },
  "postCreateCommand": "bash ./.devcontainer/postCreateCommands.sh",
  "customizations": {
		"vscode": {
			"extensions": [
        "ms-python.debugpy",
				"streetsidesoftware.code-spell-checker"
			]
		}
	},
  "containerEnv": { 
		"HOME_CT": "/workspaces/Computational-Thinking-8",
		"PYTHONPATH": "/workspaces/Computational-Thinking-8/"
	}
}