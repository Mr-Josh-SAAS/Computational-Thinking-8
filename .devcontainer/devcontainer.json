{
  "name": "Python 3",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
    "ghcr.io/devcontainers/features/desktop-lite:1": {      
			"password": "",
			"webPort": "6080",
			"vncPort": "5901"
    }
  },
  "forwardPorts": [
    6080
  ],
  "portsAttributes": {
    "6080": {
      "label": "desktop"
    }
  },
  // "postCreateCommand": "pip3 install --user -r requirements.txt",
  "customizations": {
		"vscode": {
			"extensions": [
				"streetsidesoftware.code-spell-checker"
			]
		}
	},
  "containerEnv": { 
		"HOME_CT": "/workspaces/Computational-Thinking-8",
		"PYTHONPATH": "/workspaces/Computational-Thinking-8/"
	}
}